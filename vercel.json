{
  "version": 2,
  "buildCommand": "",
  "installCommand": "",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin.html"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://fixloapp.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
