{
  "version": 2,
  "buildCommand": "cd client && npm install --loglevel=error && npm run build",
  "outputDirectory": "client/build",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://fixlo-backend.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
